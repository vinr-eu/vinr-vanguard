{
  "kind": "Environment",
  "apiVersion": "v1",
  "name": "richy-local",
  "imports": [
    "../../services/nest-js-app",
    "../../services/next-js-app",
    "../../services/spring-boot-app"
  ],
  "overrides": {
    "nest-js-app": {
      "branch": "main",
      "ingressHost": "api.vinr.local",
      "variables": [
        {
          "name": "PORT",
          "value": "3001"
        }
      ]
    },
    "next-js-app": {
      "branch": "main",
      "ingressHost": "vinr.local",
      "variables": [
        {
          "name": "PORT",
          "value": "3000"
        }
      ]
    },
    "spring-boot-app": {
      "branch": "main",
      "variables": [
        {
          "name": "SERVER_PORT",
          "value": "3002"
        },
        {
          "name": "SPRING_MONGODB_URI",
          "ref": "aws/secrets/mongo-db-url"
        }
      ]
    }
  }
}
