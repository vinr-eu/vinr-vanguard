{
  "kind": "Environment",
  "apiVersion": "v1",
  "name": "richy-local",
  "imports": [
    "../../services/nest-js-app",
    "../../services/next-js-app"
  ],
  "overrides": {
    "nest-js-app": {
      "branch": "main",
      "variables": [
        {
          "name": "PORT",
          "value": "3000"
        }
      ]
    },
    "next-js-app": {
      "branch": "main",
      "variables": [
        {
          "name": "PORT",
          "value": "3001"
        }
      ]
    }
  }
}